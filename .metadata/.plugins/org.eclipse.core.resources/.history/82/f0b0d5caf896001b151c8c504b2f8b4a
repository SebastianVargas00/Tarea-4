package Diseño;

public class Logger {

private static Logger log;

private Logger() {
	
	
}

public static Logger getInstance() {
	
	if(log==null) {
		log=new Logger();
	}
	return log;
	
}

public int log(int num1,int num2) {
	int suma=num1+num2;
	return suma;
	
	
	
	//int resta = num1-num2;
	//int division= num1/num2;
	
	
}


}
